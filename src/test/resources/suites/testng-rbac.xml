<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RBAC Suite">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>
    <test name="RBAC Tests">
        <groups>
            <run>
                <include name="rbac"/>
            </run>
        </groups>
        <packages>
            <package name="tests.authz.*"/>
        </packages>
    </test>
</suite>
